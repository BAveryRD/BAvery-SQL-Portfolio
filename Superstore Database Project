#Used this statement to get a broad visual of the data to be analyzed
SELECT * 
FROM superstore;

#Found Average price of items under the Kitchen Supplies category
SELECT AVG(price)
FROM superstore
WHERE category='Kitchen Supplies';

Found Average price of items from the Electronics category
SELECT AVG(price)
FROM superstore
WHERE category ='Electronics';

#Found item with the lowest customer rating
SELECT MIN(average_rating)
FROM superstore;

#Found product with highest custoemr rating
SELECT MAX(average_rating)
FROM superstore
